    "framer-motion": "^4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^7.20.5",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.2",
